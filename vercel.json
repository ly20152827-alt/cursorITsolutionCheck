{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "functions": {
    "api/app.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}

